    border-radius: 13px;
    border-style: none none solid;
    border-width: 0 0 2px;
    box-shadow: none;
    max-height: 100000px;
    min-height: 63px;
    padding: 20px;
    position: relative;
    transition: max-height 0.5s ease 0s;
}

#headerwrap {
    background: url("http://www.basicoh.com/demo/try/assets/css/../img/header-bg.jpg") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    max-height: 550px;
    padding-top: 0;
    text-align: left;
}

td{
  border-bottom: 1px dotted #666;
}


#quote {
    padding-bottom: 55px;
    padding-top: 55px;
}

h1#header {
    font-family: "Pacifico";
    font-size: 62px;
    padding: 50px;
    text-align: center;
    text-shadow: 0 2px 2px #3c77b3;
}
h1.text-primary {
    color: #428bca;
}

#quote {
    background-color: #5BC0DE;
}

#footer {
    background: none repeat scroll 0 0 #343434;
    color: #f2f2f2;
    padding-bottom: 30px;
    padding-top: 30px;
}
#copyright {
    background-color: #151515;
    color: #f2f2f2;
    padding-top: 15px;
}
</style>
