   <div class="col-lg-7">
<!-- http://eisenbraun.github.io/columns/ -->
     <h2>Mon coup de coeur sur le web (<%= Date.today %>)</h2> 
    <!-- List suggestions d'articles --> 

       <div class="well">
        <table>
	<% data.links.liens.each do |n| %>
           <% if n.date %>
          <tr>
            <td><h4 class="links"><a href="<%= n.link %>" target="_blank"><%= n.title %> </a>&nbsp; <span class="badge"><%= n.categorie %></span></h4></td>
          </tr>
         <% end %>
	<% end %>
        </table>
     </div>
   </div>
   




 </div><!-- fin row -->
</div>
