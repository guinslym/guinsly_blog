<body>
<div id="main" role="main" class="container">
      <%= yield %>
    </div>



  <section id="headerwrap">
  <div class="jumbotron text-center">
      <h1 class="text-primary" id="header">My Blog</h1>
    </div>
  </section>






<section id="article">

  <div class="container">

<div class="row"> 
   <div class="col-lg-9">

 <h2>Most recent articles</h2>


     <!-- begining of ruby each -->
<% blog.articles[0...10].each do |article| %>
    <div class="row">
     
      <div class="col-lg-12">
       <div class="well">

         <div class="row">
           <div class="col-lg-12">
	       <br>
             <h3><%= link_to  article.title, article %></h3>
	     <p class="text-muted"><%= article.date.strftime('%b %e') %></p>
           </div>
	</div>
     </div>

   </div><!-- offset-2 -->
  </div>

 <% end %><!-- end of .each -->
  <!--- end of ruby each -->


 </div>



   <div class="col-lg-3"> 
<h2>Tags</h2>
    <div class="well"> 
             
      <ol>
        <% blog.tags.each do |tag, articles| %>
          <li><%= link_to "#{tag} (#{articles.size})", tag_path(tag) %></li>
        <% end %>
      </ol>
   <!--
      <h2>By Year</h2>
